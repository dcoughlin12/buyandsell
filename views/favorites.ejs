<body>
<%- include('partials/_header') %>

<h1>My Favorites</h1>

<div class="container">
  <div class="row mt-500 ">
  <% for (item in myListings) { %>
    <div class="col-sm-4 mt-500">
      <div class="panel panel-primary">
        <div class="panel-heading listing-header"> <%= myListings.title %></div>
        <div class="panel-body"> <a href="/***include individual listing**"> <img src="<%= myListings.url_image %>" class="img-responsive" style="width:100%" alt="Image">
           <a href="#"> <button class="glyphicon glyphicon-heart-empty listing-button-heart"></span>
          <a href="/messages"> <button class="glyphicon glyphicon-comment listing-button-message"></span></div>
        <div class="panel-footer"></div>
      </div>
    </div>
    <% } %>
  </div>
</div>
</body>
